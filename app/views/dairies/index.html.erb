<h1>Listing dairies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Exercise</th>
      <th>Attempts</th>
      <th>Weight</th>
      <th>When</th>
      <th>Done</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dairies.each do |dairy| %>
      <tr>
        <td><%= dairy.user %></td>
        <td><%= dairy.exercise %></td>
        <td><%= dairy.attempts %></td>
        <td><%= dairy.weight %></td>
        <td><%= dairy.when %></td>
        <td><%= dairy.done %></td>
        <td><%= link_to 'Show', dairy %></td>
        <td><%= link_to 'Edit', edit_dairy_path(dairy) %></td>
        <td><%= link_to 'Destroy', dairy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dairy', new_dairy_path %>
